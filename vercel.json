{
  "buildCommand": "node scripts/deploy.js",
  "installCommand": "npm ci",
  "env": {
    "TZ": "Asia/Shanghai"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}
